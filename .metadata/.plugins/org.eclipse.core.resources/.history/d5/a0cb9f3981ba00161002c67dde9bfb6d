/**
 * Kelly Luo
 * 9/24/16
 * APCS2
 * This code accesses the Calculate Library and uses those
 * methods to carries out math problems.
 */
public class DoMath {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		System.out.println(Calculate.square(5));
		System.out.println(Calculate.cube(2));
		System.out.println(Calculate.average(2.0, 2.0));
		System.out.println(Calculate.average(2.0, 2.0, 1.0));
		System.out.println(Calculate.toDegrees(3.14));
		System.out.println(Calculate.toRadians(180));
		System.out.println(Calculate.discriminant(1, 4, 3));
		System.out.println(Calculate.toImproperFrac(3,1,2));
		System.out.println(Calculate.toMixedNum(9,2));
		System.out.println(Calculate.foil(2, 3, 6, -7, "n"));
		System.out.println(Calculate.isDivisibleBy(5, 3));
		System.out.println(Calculate.absValue(-2.0)); 
		System.out.println(Calculate.max(1, 3));
		System.out.println(Calculate.max(1, 2, 4));
		System.out.println(Calculate.min(1,2));
		System.out.println(Calculate.round2(20.34));
		System.out.println(Calculate.exponent(2, 2.0));
		System.out.println(Calculate.factorial(5));
		System.out.println(Calculate.isPrime(5));
		System.out.println(Calculate.gcf(5, 25));
		System.out.println(Calculate.sqrt(9.0));
		System.out.println(Calculate.quadForm(100, 100, 5));
		
		
	}

}
